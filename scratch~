        plt.subplot(3, 1, 1)
        plt.plot(xt, x, 'o')
        plt.axis('equal')
        plt.axis([-100,100,-100,100])
        plt.title('X of '+str(l))

        plt.subplot(3, 1, 2)
        plt.plot(yt,y,'o')
        plt.axis('equal')
        plt.axis([-100,100,-100,100])

        plt.title('Y of '+str(l))

        plt.subplot(3, 1, 3)
        plt.plot(zt,z,'o')
        plt.xlabel('True (mm)')
        plt.ylabel('Predicted (mm)')
        plt.axis('equal')
        plt.axis([-100,100,-100,100])

        plt.title('Z of '+str(l))
        plt.show()                            
